//获取当前时间
func NOWDATE(formatStr : String) -> String {
    let date = Date()
    let formatter = DateFormatter()
    formatter.dateFormat = formatStr
    formatter.locale = Locale.current
    let strNowTime = formatter.string(from: date)
    return strNowTime
}

